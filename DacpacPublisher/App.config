<?xml version="1.0" encoding="utf-8"?>

<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<appSettings>
		<!-- Default options for DACPAC Deployment -->
		<add key="DefaultBlockOnDataLoss" value="false" />
		<add key="DefaultDropObjectsNotInSource" value="true" />
		<add key="DefaultVerifyDeployment" value="true" />

		<!-- SQL Agent Jobs -->
		<add key="DefaultSqlAgentJobSchedule" value="FREQ=DAILY;INTERVAL=1;STARTTIME=000000" />

		<!-- Logging options -->
		<add key="LogFilePath" value="Publish_Log.txt" />
		<add key="LoggingEnabled" value="true" />
	</appSettings>
</configuration>